{
  "dashboard": {
    "title": "Airflow Metrics",
    "panels": [
      {
        "type": "graph",
        "title": "Task Duration",
        "targets": [
          {
            "expr": "airflow_task_duration_seconds",
            "format": "time_series"
          }
        ]
      },
      {
        "type": "graph",
        "title": "Task Failures",
        "targets": [
          {
            "expr": "airflow_task_failures_total",
            "format": "time_series"
          }
        ]
      }
    ]
  }
}